{
    "name": "kweaver00/watson_php",
    "description": "A PHP SDK for using IBM Watsons",
    "keywords": [
        "ibm", "ibm watson", "php"
    ],
    "homepage": "https://github.com/kweaver00/unofficial-ibm-watson-php-sdk",
    "license": "Apache 2.0",
    "authors": [
        {
            "name": "Keith Weaver",
            "email": "me@keithweaver.ca",
            "homepage": "https://keithweaver.ca",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "ext-curl": "*",
        "php-curl-class/php-curl-class": "^4.11"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "ext-curl": "*",
        "php-curl-class/php-curl-class": "^4.11"
    },
    "autoload": {
        "psr-4": {
            "Kweaver\\Watson\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kweaver\\Watson\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
